# Пример файла переменных окружения
# Скопируйте в .env и заполните своими значениями

# Конфигурация Flask
FLASK_ENV=development
SECRET_KEY=your-super-secret-key-here

# База данных (для PythonAnywhere)
DATABASE_URL=sqlite:///app.db

# Для MySQL на PythonAnywhere (раскомментируйте если используете MySQL)
# DATABASE_URL=mysql+pymysql://username:password@username.mysql.pythonanywhere-services.com/username$dbname

# Dart SDK (путь к исполняемому файлу dart)
DART_PATH=dart